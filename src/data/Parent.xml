<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="data.Parent">
        <table name="PARENT" schema="PUBLIC" catalog="PUBLIC"/>
        <attributes>
            <id name="idParent">
                <column name="ID_PARENT" precision="32"/>
            </id>
            <basic name="name">
                <column name="NAME" nullable="false"/>
            </basic>
            <one-to-many name="child" target-entity="data.Child">
                <join-column name="ID_PARENT" /> <!-- ERROR: Cannot resolve column id_parent! -->
            </one-to-many>
        </attributes>
    </entity>
</entity-mappings>
